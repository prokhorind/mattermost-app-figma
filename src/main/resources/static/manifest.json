{
  "app_id": "figma",
  "version": "0.7.0",
  "icon": "icon.png",
  "display_name": "Figma",
  "description": "Figma integration",
  "homepage_url": "https://github.com/prokhorind/mattermost-figma",
  "requested_permissions": [
    "act_as_bot",
    "remote_oauth2",
    "act_as_user",
    "remote_webhooks"
  ],
  "requested_locations": [
    "/command"
  ],
  "on_install": {
    "path": "/install",
    "expand": {
      "acting_user_access_token": "all"
    }
  },
  "bindings": {
    "path": "/bindings",
    "expand": {
      "acting_user": "all",
      "oauth2_app": "all",
      "oauth2_user": "all"
    }
  },
  "get_oauth2_connect_url": {
    "path": "/oauth2/connect",
    "expand": {
      "acting_user": "all",
      "oauth2_app": "all",
      "oauth2_user": "all"
    }
  },
  "on_oauth2_complete": {
    "path": "/oauth2/complete",
    "expand": {
      "oauth2_app": "all",
      "acting_user_access_token": "all"
    }
  },
  "http": {
    "root_url": "http://localhost:8080"
  },
  "assets": []
}